    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cc.zzf.ccalarmclock.AlarmClockListFragment">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
